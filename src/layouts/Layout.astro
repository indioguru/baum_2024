---
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="Astro description" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>

  <link rel="preload" href="./assets/bg_clouds.png" as="image" />
  <link rel="preload" href="./assets/bg_clouds_destock.png" as="image" />
  <link rel="preload" href="./assets/fonts/SharpGroteskMediumItalic20-Regular/SharpGroteskMediumItalic20-Regular.woff2" as="font" type="font/woff2" crossorigin="anonymous">
  <link rel="preload" href="./assets/fonts/PPNeueMontreal-Bold/PPNeueMontreal-Bold.woff2" as="font" type="font/woff2" crossorigin="anonymous">
  <link rel="preload" href="./assets/fonts/PPNeueMontreal-Book/PPNeueMontreal-Book.woff2" as="font" type="font/woff2" crossorigin="anonymous">
  <link rel="preload" href="./assets/fonts/PPNeueMontreal-Light/PPNeueMontreal-Light.woff2" as="font" type="font/woff2" crossorigin="anonymous">
  
  <body>
    <slot />
  </body>
</html>
